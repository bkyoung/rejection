{
  "name": "rejection",
  "version": "0.1.0",
  "private": true,
  "watch": {
    "test": "test/*"
  },
  "scripts": {
    "dev": "next dev",
    "dev-test": "concurrently -n next,riteway \"next dev\" \"npm:watch\"",
    "build": "next build",
