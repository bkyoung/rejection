    "next": "12.2.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.2",
    "redux-saga": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "@babel/runtime-corejs2": "^7.18.9",
    "@swc/core": "^1.2.224",
    "@swc/register": "^0.1.10",
    "concurrently": "^7.3.0",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.4",
    "npm-watch": "^0.11.0",
